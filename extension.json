{
	"name": "ical",
	"author": [
		"Jan-Philipp Litza",
		"Kjell-Inge Gustafsson",
		"Yitzchok Lavi",
		"Rami Lehti"
	],
	"license-name": "GPL",
	"url": "https://www.mediawiki.org/wiki/Extension:Ical",
	"descriptionmsg": "ical-desc",
	"type": "parserhook",
	"requires": {
		"MediaWiki": ">= 1.35.0"
	},
	"config": {
		"IcalTimeFormat": {
			"value": "%H:%M"
		},
		"IcalDateFormat": {
			"value": "%a, %d. %b %Y"
		},
		"IcalShortDateFormat": {
			"value": "%d.%m."
		},
		"IcalDaysToShow": {
			"value": "21"
		},
		"IcalIcsLink": {
			"value": "false"
		},
		"IcalRefreshLink": {
			"value": "false"
		},
		"IcalCaption": {
			"value": "false"
		}
	},
	"MessagesDirs": {
		"Ical": [
			"i18n"
		]
	},
	"Hooks": {
		"ParserFirstCallInit": [
			"Ical::init"
		]
	},
	"AutoloadClasses": {
		"Ical": "includes/ical.php",
		"vcalendar": "includes/iCalcreator.class.php"
	},
	"manifest_version": 2
}

